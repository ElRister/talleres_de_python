    "    Resultado = \"par\"\n",
    "  else:\n",
    "    Resultado = \"impar\"\n",
    "  return Resultado\n",
    "\n",
    "Resultado = Definir_par(Num)\n",
    "print(f\"El número {Num} es {Resultado}.\")"
